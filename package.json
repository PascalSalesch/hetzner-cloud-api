{
  "name": "@pascalsalesch/hetzner-cloud-api",
  "version": "1.0.0",
  "description": "JavaScript client for the Hetzner Cloud API.",
  "module": "api.mjs",
  "scripts": {
    "test": "c8 run-s --print-label \"test:**\"",
    "test:lint": "standard",
    "test:api": "mocha --timeout 25000 \"tests/**/*.mjs\""
  },
  "keywords": [
    "Hetzner",
    "Cloud",
    "API"
  ],
  "author": "Pascal Salesch <pascal.b.salesch@gmail.com>",
  "license": "MIT",
  "c8": {
    "report-dir": "./node_modules/c8/.coverage",
    "check-coverage": true,
    "lines": 0,
    "functions": 0,
    "branches": 0
  },
  "devDependencies": {
    "c8": "^7.11.0",
    "chai": "^4.3.6",
    "mocha": "^9.2.0",
    "npm-run-all": "^4.1.5",
    "standard": "^16.0.4"
  },
  "dependencies": {
    "node-fetch": "^3.2.0"
  }
}
